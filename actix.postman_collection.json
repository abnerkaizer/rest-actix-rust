        {
          "name": "login admin",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "// Parse the JSON response",
                  "const jsonData = pm.response.json();",
                  "",
                  "// Extract the token and save it to the environment variable",
                  "const token = jsonData.token;",
                  "pm.environment.set(\"token\", token);",
                  "",
                  "// Test to verify the token was successfully saved",
                  "pm.test(\"Token was successfully extracted and saved to environment\", function () {",
                  "    const savedToken = pm.environment.get(\"token\");",
                  "    pm.expect(savedToken).to.be.a(\"string\");",
                  "    pm.expect(savedToken).to.not.be.empty;",
                  "    pm.expect(savedToken).to.eql(token);",
                  "});"
                ],
                "type": "text/javascript",
                "packages": {},
                "requests": {}
              }
            }
