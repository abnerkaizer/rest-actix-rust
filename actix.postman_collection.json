                  "}",
                  "",
                  "// Parse the JSON response with error handling",
                  "let jsonResponse;",
                  "try {",
                  "    jsonResponse = pm.response.json();",
                  "    console.log(\"Parsed JSON response:\", JSON.stringify(jsonResponse, null, 2));",
                  "} catch (parseError) {",
                  "    console.error(\"ERROR: Failed to parse JSON response:\", parseError.message);",
                  "}",
                  "",
                  "// Check if token exists in the response",
                  "if (jsonResponse) {",
                  "    console.log(\"Token exists in response:\", jsonResponse.hasOwnProperty('token'));",
                  "    console.log(\"Token value:\", jsonResponse.token);",
                  "    console.log(\"Token type:\", typeof jsonResponse.token);",
                  "    ",
                  "    if (jsonResponse.token) {",
                  "        // Set the token as a collection variable",
                  "        pm.collectionVariables.set(\"token\", jsonResponse.token);",
                  "        console.log(\"SUCCESS: Token set in collection variable\");",
                  "        ",
                  "        // Verify the variable was set",
                  "        const verifyToken = pm.collectionVariables.get(\"token\");",
                  "        console.log(\"Verification - Token retrieved from collection variables:\", verifyToken ? \"Yes (length: \" + verifyToken.length + \")\" : \"No\");",
                  "    } else {",
                  "        console.error(\"ERROR: Token is null, undefined, or empty in the response\");",
                  "        console.log(\"Available keys in response:\", Object.keys(jsonResponse));",
                  "    }",
                  "} else {",
                  "    console.error(\"ERROR: jsonResponse is null or undefined\");",
                  "}",
                  "",
                  "// Debug: Check current collection variables info",
                  "console.log(\"=== Collection Variables Debug Info ===\");",
                  "console.log(\"All collection variables:\", JSON.stringify(pm.collectionVariables.toObject(), null, 2));",
                  "console.log(\"=== DEBUG: Post-Response Script Completed ===\");"
